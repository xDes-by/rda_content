<root> 
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="file://{resources}/layout/custom_game/talents/talents.css" />
		<include src="file://{resources}/layout/custom_game/talents/buy_confirmation.css" />
	</styles>

	<scripts>
		<include src="file://{resources}/layout/custom_game/talents/talents_variables.js" />
		<include src="file://{resources}/layout/custom_game/talents/talents_change_hud.js" />
		<include src="file://{resources}/layout/custom_game/talents/talents_update_icons.js" />
		<include src="file://{resources}/layout/custom_game/talents/talents_view_processing.js" />
		<include src="file://{resources}/layout/custom_game/talents/buy_confirmation.js" />
		<include src="file://{resources}/layout/custom_game/talents/talents_functions.js" />
	</scripts>

    <snippets>
		<snippet name="heroModel">
            <Panel>
                <DOTAScenePanel unit = "npc_dota_hero_axe" particleonly = "false" allowrotation ="true" class = "" />
            </Panel>
		</snippet>
        <snippet name="item_Panel">
            <Panel id="" class="item_Panel">
                <Image id="item_image" class="item_image" src="file://{resources}/images/custom_game/talants/scroll1.png" />
                <Label id="item_name" class="item_name" text="#talant-shop-refresh" />
                <Label id="item_count" class="item_count" text="" />
                <Panel id="item_use_button_panel" class="item_use_button_panel">
                    <Label id="" class="" text="#talents_use_button_label" />
                </Panel>
                <Panel id="" class="buy_buttons_panel">
                    <Panel id="don_button" class="buy_button">
                        <Panel id="" class="" style="align:center center;flow-children:right;">
                            <Panel id="don_icon" class=""/>
                            <Label id="don_label" class="buy_button_label" text="10" />
                        </Panel>
                    </Panel>
                    <Panel id="rp_button" class="buy_button">
                        <Panel id="" class="" style="align:center center;flow-children:right;">
                            <Panel id="rp_icon" class=""/>
                            <Label id="rp_label" class="buy_button_label" text="10" />
                        </Panel>
                    </Panel>
                </Panel>
            </Panel>
        </snippet>
	</snippets>

	<Panel class = "root" hittest="false">
        <!-- <Image class='talante_bg_image_panel' id='talante_bg_image_panel' src="file://{resources}/images/custom_game/RDAShop/spray/gems_reward_750.png" /> -->
        <Panel id="talant_root" class="talant_root">
            <DOTAScenePanel id="panel_bg" class = "panel_bg" map="scene2" light="light_1" camera="camera_1" particleonly="false" hittest="true" onmouseactivate="CloseButton()" />
            <!-- <DOTAScenePanel id='hero_scen_panel' class='HeroScenePanel' style='width:800px;height:800px;margin-right:10px;' unit='' particleonly='false' allowrotation='true' /> -->
            <Panel id="cheat_mode_panel" class="HeroScenePanel" hittest="false">
                <Label id="cheat_mode_label" class="" text="#talents_cheat_mode_label" hittest="false" />
                <Panel id="button_cheat_drop" class="" onmouseactivate="RemoveAllTalents()">
                    <Label id="" class="" text="#talents_cheat_drop_label" />
                </Panel>
            </Panel>
            <Panel id="activate_second_branch_panel" class="HeroScenePanel" hittest="false">
                <Label id="activate_second_branch_label" class="" text="#talents_activate_second_branch" hittest="false"/>
                <Panel id="activate_second_branch_button_background" class="" style="flow-children: right;">
                    <Panel id="activate_second_branch_button_don" style="width: 50%;height: 100%;">
                        <Panel style="align: center center;flow-children: right;">
                            <Panel id="don_icon" class=""/>
                            <Label id="don_label" class="buy_button_label" text="200" />
                        </Panel>
                    </Panel>
                    <Panel id="activate_second_branch_button_rp" style="width: 50%;height: 100%;">
                        <Panel style="align: center center;flow-children: right;">
                            <Panel id="rp_icon" class=""/>
                            <Label id="rp_label" class="buy_button_label" text="1000" />
                        </Panel>
                    </Panel>
                </Panel>
            </Panel>
            
            <Panel class="smoke_movie_panel" />

            <Panel id="talant_tree_panel" class="talant_tree_panel" onmouseactivate="">
                
                <Image class='talante_bg_image_panel' id='talante_bg_image_panel' src="file://{resources}/images/custom_game/talants/talents_tree_v2.png" />
                <Panel id="talant_tree_images_panel" class="talant_tree_images_panel"/>

            </Panel>

            <Panel id="description_panel" class="description_panel" onmouseactivate="">
                <Panel id="description_gradient_border_out" class="description_gradient_border_out">
                    <Image id="arrow_right" class="" src="file://{resources}/images/custom_game/talants/profilecard_tooltip_arrow_right_png.png" />
                    <Image class='background-image-description' id='background-image-description' src="file://{resources}/images/custom_game/talants/111.png" />
                    <Image class='talant_img_description' id='talant_img_description' src="" />
                    <Label id="talant_name_label" class="talant_name_label" text="" />
                    <Label id="talant_description_label" class="talant_description_label" text="#tooltip_talant_30_level" />
                    <Panel id="under_line_panel" class="under_line_panel">
                        <Label id="talant_description_buff_label" class="talant_description_buff_label" text="" html="true" />
                        <Label id="talant_description_scale_label" class="talant_description_scale_label" text="200/250/300/350/400" html="true" />
                    </Panel>
                    <Image class='button' id='button' src="file://{resources}/images/custom_game/talants/button_thin_secondary_png.png" >
                        <Panel id="" class="center flowRight">
                            <Label id="button_label" class="button_label" text="#talant_button_examine" />
                            <Label id="button_label_count" class="button_label" text="(5)" />
                        </Panel>
                    </Image>
                    <Label id="players_have" class="players_have_result" text="есть у 23 игроков" />
                    <Panel id="InfoIcon" class="" onload="$('#InfoIcon').visible = false"/>
                </Panel>
                <Panel id="talant-2nd-window" class="talant-2nd-window" onmouseactivate="">
                    <Image id="arrow_left" class="" src="file://{resources}/images/custom_game/talants/profilecard_tooltip_arrow_left_png.png" />
                    <Image class='' id='talant-bg-img-shop' src="file://{resources}/images/custom_game/talants/2w3s.png" />
                    <Panel id="player-state" class="player-state">
                        <!-- <Panel id="" class="state">
                            <Label id="" class="state-text" text="#talant_normal_exp_label" />
                            <Label id="state-blue-exp" class="state-text" text="124242" />
                        </Panel>
                        <Panel id="" class="state">
                            <Label id="" class="state-text" text="#talant_don_exp_label" />
                            <Label id="state-gold-exp" class="state-text" text="124242" />
                        </Panel>
                        <Panel id="" class="state">
                            <Label id="" class="state-text" text="#talant_game_cout_label" />
                            <Label id="state-games" class="state-text" text="124242" />
                        </Panel>
                        <Panel id="" class="state">
                            <Label id="" class="state-text" text="#talant_game_time_label" />
                            <Label id="state-time" class="state-text" text="124242" />
                        </Panel> -->
                    </Panel>
                    <Panel id="talant-shop" class="talant-shop">
                        
                    </Panel>
                </Panel>
            </Panel>

            <Panel id="progress_line_panel" class="progress_line_panel" onmouseactivate="">
                <Panel id="exp_line_container" class="exp_line_container">
                    <Panel id="blue_line_panel" class="blue_line_panel">
                        <DOTAScenePanel id="top_burner" class="SceneLoaded" map="maps/scenes/hud/healthbarburner.vmap" camera="camera_1" />
                    </Panel>
                    <Panel id="red_line_panel" class="red_line_panel">
                        <DOTAScenePanel id="botton_burner" class="SceneLoaded" map="maps/scenes/hud/healthbarburner.vmap" camera="camera_1" />
                    </Panel>
                </Panel>
                <Panel id="exp_line_container2" class="exp_line_container2">
                    <Label id="blue_line_label" class="blue_line_label" text="" />
                    <Label id="red_line_label" class="red_line_label" text="" />
                </Panel>
                <Panel id="gain_container" class="gain_container">
                    <Label id="blue_line_label_gain" class="blue_line_label_gain" text="" />
                    <Label id="red_line_label_gain" class="red_line_label_gain" text="" />
                </Panel>
                <Panel id="level_info_panel" class="level_info_panel">
                    <Label id="RDA_level_label" class="RDA_level_label" text="" />
                    <Label id="normal_level_label" class="normal_level_label" text="" />
                </Panel>
                <Image class='progress_line_bg' id='progress_line_bg' src="file://{resources}/images/custom_game/talants/exp_contur2.png" />
                

            </Panel>
            <!-- <Image class='openbtn' id='openbtn' onmouseactivate="opnShop();" src="file://{resources}/images/custom_game/talants/btnmore.png" /> -->

            <Panel id="close_panel_bg" class="close_panel_bg" >
                <!--<Panel class='close_exit_img' id='close_exit_img' src="file://{resources}/images/custom_game/talants/exit.png" />-->
                <Image class='close_button' id='close_button' onmouseactivate="CloseButton()" src="file://{resources}/images/custom_game/talants/button_thin_secondary_png.png" />
                <Label id="" class="" text="#close" />
            </Panel>
            <Panel id="exp_boost_panel" class="">
                <Label id="exp_boost_lavel_2" class="" text="x3.0" />
            </Panel>
            <Panel id="buy_coufirm_bg" class="" onactivate="HideConfirmPanel()">
                <Panel id="buy_confirm_panel" class="" onactivate="">
                    <Panel id="buy_description_panel" class="">
                        <Panel id="" class="" style="flow-children: right; horizontal-align: center;">
                            <Label id="buy_confirm_item_name" class="buy_description_text" html="true" text="корм" />
                        </Panel>
                        <Panel id="" class="" style="flow-children: right; horizontal-align: center; margin-top:10px;">
                            <Label id="buy_confirm_price" class="buy_description_text" text="300" />
                            <Panel id="coufirm_rp_image_pet" class=""></Panel>
                            <Panel id="coufirm_gems_image_pet" class=""></Panel>
                        </Panel>
                        <DropDown id="confirm_drop_down" class="">
                            <Label text="1 шт" id="entry1"/>
                            <Label text="2 шт" id="entry2"/>
                            <Label text="3 шт" id="entry3"/>
                            <Label text="4 шт" id="entry4"/>
                        </DropDown>
                    </Panel>
                    <Panel id="confirm_yes_panel" class="confirm_panel">
                        <Label id="" class="" text="#yes" />
                    </Panel>
                    <Panel id="confirm_no_panel" class="confirm_panel" onactivate="HideConfirmPanel()">
                        <Label id="" class="" text="#no" />
                    </Panel>
                </Panel>
            </Panel>
        </Panel>
    </Panel>
</root>
