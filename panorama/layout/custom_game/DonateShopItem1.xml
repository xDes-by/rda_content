<root>
	<styles>
        <include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="file://{resources}/styles/custom_game/DonateShop.css" />
	</styles>
	<Panel class = "DonateShopItem1">
		

		<DOTAItemImage id='DonateShopItem' class="DonateShopItem" hittest="true" itemname="">
			<Image id='DonateShopImg' class="DonateShopImg" src=""/>
			<Panel id="DonateShopItemButtonLabelStockPanel" class="DonateShopItemButtonLabelStockPanel">
				<Label id ='DonateShopItemButtonLabelStock' class="DonateShopItemButtonLabelStock" text='50'/>
			</Panel>
			<ToggleButton id="ShopItemCheckBox" class="ShopItemCheckBox" text="" />
		</DOTAItemImage>
		<Panel id='DonateShopItemLabel_c'>
			<Label id = "DonateShopItemLabel" class="DonateShopItemLabel" text='Аганим'/>
		</Panel>

		<Panel id = 'DonateShopItemButtonBuy' class="DonateShopItemButtonBuy">
			<Panel id="buy_line_pn" >
				<Panel id="shop_button_price_container_don" class="">
					<Panel class='shopButtonImgAndText' id='shopButtonImgAndText1'>
						<Label id='DonateShopItemButtonLabelNotAlign1' class="DonateShopItemButtonLabelNotAlign" text='500'/>
						<Image class='shopButtonImg' id='shopButtonImg1' src="file://{resources}/images/custom_game/DonateShop/money_logo_3_mini.png" />
					</Panel>
				</Panel>
				<Panel id="shop_button_price_container_rp" class="">
					<Panel class='shopButtonImgAndText' id='shopButtonImgAndText2'>
						<Label id='DonateShopItemButtonLabelNotAlign2' class="DonateShopItemButtonLabelNotAlign" text='500'/>
						<Image class='shopButtonImg' id='shopButtonImg2' src="file://{resources}/images/custom_game/DonateShop/protection_mini.png" />
					</Panel>
				</Panel>
			</Panel>
		</Panel>
		<Panel id = "DonateShopItemButtonHas" class="DonateShopItemButtonHas">
			<Label id = 'DonateShopItemButtonLabel' class="DonateShopItemButtonLabel" text='#taik'/>
		</Panel>
		<Panel id = "DonateShopItemButtonGived" class="DonateShopItemButtonGived">
			<Label id = 'DonateShopItemButtonLabelGived' class="DonateShopItemButtonLabelGived" text='#taik'/>
		</Panel>
		<Panel id = "DonateShopItemButtonActive" class="DonateShopItemButtonActive">
			<Label id = 'DonateShopItemButtonLabelActive' class="DonateShopItemButtonLabelActive" text='Выдано'/>
		</Panel>
		<Panel id = "DonateShopItemButtonInTreasuries" class="DonateShopItemButtonInTreasuries">
			<Label id = 'DonateShopItemButtonInTreasuriesLabel' class="DonateShopItemButtonInTreasuriesLabel" text='#InTreasuriesLabel'/>
		</Panel>
		<Panel id = "DonateShopItemButtonSell" class="DonateShopItemButtonSell">
			<Panel style="flow-children:right; align: center center; height:100%;">
				<Label id='DonateShopItemButtonSellLabel' class="DonateShopItemButtonSellLabel" text='Продать за 10'/>
				<Image class='DonateShopItemButtonSellImg' id='DonateShopItemButtonSellImg' src="file://{resources}/images/custom_game/DonateShop/protection_mini.png" />
			</Panel>
		</Panel>
	</Panel>
</root>