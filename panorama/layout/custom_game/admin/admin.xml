<root>

	<styles>
        <include src="file://{resources}/layout/custom_game/admin/admin.css" />
        <include src="s2r://panorama/styles/dotastyles.vcss_c" />
    </styles>
    <scripts>
        <include src="file://{resources}/layout/custom_game/admin/admin.js" />
    </scripts>
	
	
    <Panel class ="root" hittest="false">
        
        <Panel id="main_panel" class="">
            <Label id="admin_panel_label" class="" text="Cheat Panel" />
            <Panel id="parameter_panel" class="">
                <Panel id="" class="parameter_lines">
                    <Panel id="" class="button_panel" oncontextmenu = "ChangeGold(-10000)" onmouseactivate = "ChangeGold(10000)">
                        <Label id="" class="font_class font_align" text="10K золота"/>
                    </Panel>
                    <Panel id="" class="button_panel" oncontextmenu = "ChangeGold(-100000)" onmouseactivate = "ChangeGold(100000)">
                        <Label id="" class="font_class font_align" text="100K золота"/>
                    </Panel>
                    <Panel id="" class="button_panel" oncontextmenu = "ChangeGold(-500000)" onmouseactivate = "ChangeGold(500000)">
                        <Label id="" class="font_class font_align" text="500K золота"/>
                    </Panel>
                    <Panel id="" class="button_panel" oncontextmenu = "ChangeGold(-1000000)" onmouseactivate = "ChangeGold(1000000)">
                        <Label id="" class="font_class font_align" text="1M золота"/>
                    </Panel>
                </Panel>
                <Panel id="" class="parameter_lines">
                    <Panel id="" class="button_panel" onmouseactivate = "ChangeLevel(10)">
                        <Label id="" class="font_class font_align" text="+10 уровней" />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "ChangeLevel(100)">
                        <Label id="" class="font_class font_align" text="+100 уровней"  />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "Talents()">
                        <Label id="" class="font_class font_align" text="Таланты"  />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "Localize()">
                        <Label id="" class="font_class font_align" text="Локализация"  />
                    </Panel>
                </Panel>
                <Panel id="" class="parameter_lines">
                    <Panel id="" class="button_panel" onmouseactivate = "DropItems(8)">
                        <Label id="" class="font_class font_align" text="Предметы LVL 8"  />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "DropItems(10)">
                        <Label id="" class="font_class font_align" text="Предметы LVL 10"  />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "GiveSouls()">
                        <Label id="" class="font_class font_align" text="Души"  />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "GiveBooks()">
                        <Label id="" class="font_class font_align" text="Книги"  />
                    </Panel>
                </Panel>
                <Panel id="" class="parameter_lines">
                    <Panel id="" class="button_panel" onmouseactivate = "ChangeGems(10000)">
                        <Label id="" class="font_class font_align" text="+10К камней" />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "BattlePassAddExperience(666)">
                        <Label id="" class="font_class font_align" text="+666 БП" />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "BattlePassDrop()">
                        <Label id="" class="font_class font_align" text="Сброс БП" />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "AddCoins()">
                        <Label id="" class="font_class font_align" text="+100 КРИСТААЛОВ" />
                    </Panel>
                </Panel>
                <Panel id="" class="parameter_lines">
                    <Panel id="" class="button_panel" onmouseactivate = "BattlePassPremium()">
                        <Label id="" class="font_class font_align" text="Premium" />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "CreateBot()">
                        <Label id="" class="font_class font_align" text="Создать бота" />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "">
                        <Label id="" class="font_class font_align" text="" />
                    </Panel>
                    <Panel id="" class="button_panel" onmouseactivate = "TEST()">
                        <Label id="" class="font_class font_align" text="TEST" />
                    </Panel>
                </Panel>
            </Panel>
        </Panel>
	</Panel>
</root>